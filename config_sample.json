{    
    "server_port": 8001,
    "device_model": "streamdock",
    "text_setting": {
        "max_lines": 5,
        "fonts": {
            "tiny_font": 16,
            "small_font": 18,
            "medium_font": 20,
            "bold_font": 24
        }
    },    
    "plugins":[
        {
            "type": "UptimePlugin", 
            "url": "<your_url>",
            "username": "",
            "password": ""
